<w:document xmlns:w="http://schemas.openxmlformats.org/wordprocessingml/2006/main">
  <w:body>
    <w:p>
      <w:r>
        <w:t>基本信息</w:t>
      </w:r>
    </w:p>
    <w:table>
      <w:tr>
        <w:tc><w:p><w:r><w:t>姓名</w:t></w:r></w:p></w:tc>
        <w:tc><w:p><w:r><w:t>{name}</w:t></w:r></w:p></w:tc>
      </w:tr>
      <w:tr>
        <w:tc><w:p><w:r><w:t>出生信息</w:t></w:r></w:p></w:tc>
        <w:tc><w:p><w:r><w:t>{birth_year}</w:t></w:r></w:p></w:tc>
      </w:tr>
      <w:tr>
        <w:tc><w:p><w:r><w:t>联系方式</w:t></w:r></w:p></w:tc>
        <w:tc><w:p><w:r><w:t>{phone}</w:t></w:r></w:p></w:tc>
      </w:tr>
      <w:tr>
        <w:tc><w:p><w:r><w:t>工作单位</w:t></w:r></w:p></w:tc>
        <w:tc><w:p><w:r><w:t>{work_experience}</w:t></w:r></w:p></w:tc>
      </w:tr>
    </w:table>

    <w:p>
      <w:r>
        <w:t>教育经历</w:t>
      </w:r>
    </w:p>
    <w:table>
      <w:tr>
        <w:tc><w:p><w:r><w:t>起止时间</w:t></w:r></w:p></w:tc>
        <w:tc><w:p><w:r><w:t>学校</w:t></w:r></w:p></w:tc>
        <w:tc><w:p><w:r><w:t>专业</w:t></w:r></w:p></w:tc>
      </w:tr>
      <w:tr>
        <w:tc><w:p><w:r><w:t>{education_experience.start}</w:t></w:r></w:p></w:tc>
        <w:tc><w:p><w:r><w:t>{education_experience.school}</w:t></w:r></w:p></w:tc>
        <w:tc><w:p><w:r><w:t>{education_experience.major}</w:t></w:r></w:p></w:tc>
      </w:tr>
    </w:table>

    <w:p>
      <w:r>
        <w:t>奖励荣誉</w:t>
      </w:r>
    </w:p>
    <w:table>
      <w:tr>
        <w:tc><w:p><w:r><w:t>时间</w:t></w:r></w:p></w:tc>
        <w:tc><w:p><w:r><w:t>荣誉</w:t></w:r></w:p></w:tc>
      </w:tr>
      <w:tr>
        <w:tc><w:p><w:r><w:t>{achievements.date}</w:t></w:r></w:p></w:tc>
        <w:tc><w:p><w:r><w:t>{achievements.title}</w:t></w:r></w:p></w:tc>
      </w:tr>
    </w:table>

    <w:p>
      <w:r>
        <w:t>其他信息</w:t>
      </w:r>
    </w:p>
    <w:table>
      <w:tr>
        <w:tc><w:p><w:r><w:t>研究方向</w:t></w:r></w:p></w:tc>
        <w:tc><w:p><w:r><w:t>{research_direction}</w:t></w:r></w:p></w:tc>
      </w:tr>
      <w:tr>
        <w:tc><w:p><w:r><w:t>其他成果</w:t></w:r></w:p></w:tc>
        <w:tc><w:p><w:r><w:t>{otherachievements}</w:t></w:r></w:p></w:tc>
      </w:tr>
    </w:table>
  </w:body>
</w:document>
